<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:orientation="vertical">

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:divider="@drawable/fengexian"
        android:orientation="horizontal"
        android:showDividers="beginning|end|middle">

        <TextView
            android:id="@+id/txt_service_name"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#FFEFEF"
            android:gravity="center"
            android:text=""
            android:textColor="#181818"
            android:textSize="13sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/txt_service_type"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#FFEFEF"
            android:gravity="center"
            android:text=""
            android:textColor="#181818"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/txt_service_number"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#FFEFEF"
            android:gravity="center"
            android:text=""
            android:textColor="#181818"
            android:textSize="13sp" />


        <TextView
            android:id="@+id/txt_service_status"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#FFEFEF"
            android:gravity="center"
            android:text=""
            android:textColor="#181818"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/txt_service_click"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#FFEFEF"
            android:gravity="center"
            android:text="立即下单"
            android:textColor="#da0a0a"
            android:textSize="13sp" />


    </TableRow>

</TableRow>